<?xml version="1.0"?>
<launch>
    <!-- Move Base -->
    <node name="move_base" pkg="move_base" type="move_base" respawn="false" output="screen">
        <param name="base_global_planner" value="navfn/NavfnROS" />
        <param name="base_local_planner" value="base_local_planner/TrajectoryPlannerROS"/>
        <param name="transform_tolerance" value="0.2"/>
        <rosparam file="$(find hurba_navigation)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find hurba_navigation)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find hurba_navigation)/config/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find hurba_navigation)/config/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find hurba_navigation)/config/base_local_planner_params.yaml" command="load" />
    </node>

</launch>